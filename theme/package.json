{
  "name": "gatsby-wordpress-preview",
  "private": true,
  "description": "A starter for WordPress + Gatsby that supports live previews. wordpress running in a docker container",
  "version": "0.3.1",
  "author": "Martin Anderen <marander@pm.me>",
  "dependencies": {
    "apollo-boost": "^0.1.20",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-link": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-link-http": "^1.5.15",
    "gatsby": "^2.13.29",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-source-graphql-universal": "^3.1.4",
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "prettier": "^1.18.2"
  },
  "homepage": "https://github.com/webmaeistro/gatsby-wordpress-preview",
  "keywords": ["gatsby", "WordPress", "Preview"],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webmaeistro/gatsby-wordpress-preview.git"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -p 3000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "docker-build": "docker-compose up -d --build"
  }
}
